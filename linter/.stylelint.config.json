{
    "extends": ["stylelint-config-standard", "stylelint-config-prettier-scss"],
    "cache": ".stylelint.cache",
    "cacheLocation": "linter/",
    "plugins": ["stylelint-scss"],
    "rules": {
        "at-rule-no-unknown": null,
        "color-hex-length": "long",
        "scss/at-rule-no-unknown": true
    }
}
